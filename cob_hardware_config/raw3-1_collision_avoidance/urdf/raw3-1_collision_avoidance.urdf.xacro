<?xml version="1.0" ?>
<robot name="raw3-1_collision_avoidance" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find ipa_order_picking_arm_planning)/robot_conf/urdf/raw3-1.urdf.xacro" />
	<xacro:include filename="$(find ipa_order_picking_arm_planning)/robot_conf/urdf/workspace_components.xacro"/>


	<!-- instantiating workcell definitions -->
	<xacro:workcell_zone3_components/>
	<!-- end of workcell definitions -->

	<link name="odom_combined">
	</link>

	<joint name="world_to_stand" type="planar">
		<parent link="odom_combined"/>
		<child link="world_frame"/>
		<origin xyz="-0.238 0 0"/>
	</joint>

	<joint name="world_to_base_padding" type="planar">
		<parent link="odom_combined"/>
		<child link="base_footprint"/>
		<origin xyz="0 0 0"/>
	</joint>


	<!-- end of arm-gripper coupling joint definitions -->
</robot>
